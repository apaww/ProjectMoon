config_version=5

[application]

config/name="Above the skies"
run/main_scene="res://menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=24
config/icon="res://cursor (3).png"
config/windows_native_icon="res://icon.ico"

[autoload]

Data="*res://src/State.gd"
DiscordRPCLoader="*res://addons/discord-rpc-gd/nodes/discord_autoload.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[dialogue_manager]

general/states=["Data"]

[display]

window/size/viewport_width=854
window/size/viewport_height=480
window/size/mode=2
window/stretch/mode="viewport"
window/vsync/vsync_mode=0
mouse_cursor/custom_image="res://cursor (3).png"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/discord-rpc-gd/plugin.cfg", "res://addons/proton_scatter/plugin.cfg")

